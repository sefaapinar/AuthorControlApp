@model List<MakaleModel.Kullanici>
@{
    Layout = "~/Views/Shared/_siteLayout.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-md-12">

            <table class="table table-hover">

                <thead>
                    <tr>
                        <th>No</th>
                        <th>Ad</th>
                        <th>Soyad</th>
                        <th>DogumTarihi</th>
                        <th>Cinsiyet</th>
                        <th>Eposta</th>
                        <th>TcKimlikNo</th>
                        <th>KullaniciAdi</th>
                        <th>Parola</th>
                        <th>UnvanNo</th>
                        <th>BolumNo</th>
                    </tr>
                </thead>

                <tbody>
                    @foreach (var kullanici in Model)
                    {
                        <tr>
                           <td>@kullanici.No</td>
                           <td>@kullanici.Ad</td>
                           <td>@kullanici.Soyad</td>
                           <td>@kullanici.DogumTarihi</td>
                           <td>@kullanici.Cinsiyet</td>
                           <td>@kullanici.Eposta</td>
                           <td>@kullanici.TcKimlikNo</td>
                           <td>@kullanici.Parola</td>
                           <td>@kullanici.UnvanNo</td>
                           <td>@kullanici.BolumNo</td>
                        </tr>

                    }

                </tbody>

            </table>

        </div>

    </div>

</div>