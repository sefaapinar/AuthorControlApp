@model List<MakaleModel.Kullanici>

<div class="container">
    <div class="row">
        <div class="col-md-3">
            <a href="">MAKALE</a>


            <table class=" table table-hover">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Ad </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var kullanici in Model)
                    {
                        <tr>
                            <td>@kullanici.Ad</td>
                            <td>@kullanici.Soyad</td>

                        </tr>
                    }

                </tbody>
            </table>
        </div>


    </div>
</div>